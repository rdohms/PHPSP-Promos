<h1>Prize list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Quantity</th>
            <th>Picture</th>
            <th>Sponsor</th>
            <th>Description</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('souphpsp_admin_prize_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.name }}</td>
            <td>{{ entity.quantity }}</td>
            <td>{{ entity.picture }}</td>
            <td>{{ entity.sponsor }}</td>
            <td>{{ entity.description }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('souphpsp_admin_prize_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('souphpsp_admin_prize_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('souphpsp_admin_prize_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
