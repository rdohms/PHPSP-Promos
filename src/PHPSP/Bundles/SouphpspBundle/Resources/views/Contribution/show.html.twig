<h1>Contribution</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Userid</th>
            <td>{{ entity.userId }}</td>
        </tr>
        <tr>
            <th>Project</th>
            <td>{{ entity.project }}</td>
        </tr>
        <tr>
            <th>Type</th>
            <td>{{ entity.type }}</td>
        </tr>
        <tr>
            <th>Description</th>
            <td>{{ entity.description }}</td>
        </tr>
        <tr>
            <th>Reference</th>
            <td>{{ entity.reference }}</td>
        </tr>
        <tr>
            <th>Notes</th>
            <td>{{ entity.notes }}</td>
        </tr>
        <tr>
            <th>Status</th>
            <td>{{ entity.status }}</td>
        </tr>
        <tr>
            <th>Mentorid</th>
            <td>{{ entity.mentorId }}</td>
        </tr>
        <tr>
            <th>Dateadded</th>
            <td>{{ entity.dateAdded|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Dateapproved</th>
            <td>{{ entity.dateApproved|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('contribution') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('contribution_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('contribution_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
